    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules/**",
      "test/**",
      "src/Logger.ts"
    ]
  },
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Type,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon build/server/src/server.js -q\" --kill-others",
    "build": "npm run clean && npm run build-ts",
    "clean": "rimraf build",
    "build-ts": "tsc --p tsconfig.json",
    "buildAndStart": "npm run build && npm run start",
    "watch-ts": "tsc -w --p tsconfig.json",
    "test": "nyc --reporter=text mocha test/**/*.ts --exit",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  }
}
