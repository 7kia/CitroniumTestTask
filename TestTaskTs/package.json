    "debug": "node build.js && backpack dev --inspect-brk | pino -o 'short'",
    "test": "node build.js && nyc --reporter=text mocha test/**/*.ts --exit",
    "grunt": "grunt"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "pg-promise": "^8.4.5",
    "pino": "^4.17.5",
    "source-map-support": "^0.4.15",
    "swagger-express-middleware": "^1.0.0-alpha.12"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.22",
    "@types/chai": "^4.1.2",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.48",
    "@types/pino": "^4.7.1",
    "awesome-typescript-loader": "^3.5.0",
    "backpack-core": "^0.4.3",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^5.3.0",
    "grunt": "^1.0.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.21",
    "grunt-typescript": "^0.8.0",
    "mocha": "^4.0.1",
    "nyc": "^12.0.2",
    "shelljs": "^0.7.8",
    "source-map-support": "^0.5.6",
    "supertest": "^3.1.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**"
    ]
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
